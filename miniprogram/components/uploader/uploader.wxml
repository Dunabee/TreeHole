<!--components/uploader/uploader.wxml-->
<view class="uploader-cell">
	<!--展示图片-->
	<view class="uploader-item" wx:for="{{uploaderList}}" wx:key="{{index}}">
		<icon class="uploader-item-icon" bindtap="clearImg" data-index="{{index}}" type="clear" size="20" color="red" />
		<image bindtap="showImg" data-index="{{index}}" src="{{item}}"></image>
	</view>
	<!--添加图片-->
	<view class="uploader" bindtap="upload" wx:if="{{showUploader}}"></view>
</view>